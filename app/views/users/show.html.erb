<div class="page-header">
  <h3><%= @user.username %></h3>
</div>

<div class="row">
  <table>
    <% @photos.each do |photo| %>
      <tr>
        <th>
          <h5><%= @user.username %></h5><h6><%= photo.created_at.in_time_zone("Central Time (US & Canada)").strftime("%l:%M %p") %></h6>
        </th>
      </tr>
      <tr>
        <td>
          <img src="<%= photo.image %>" height="300"/>
        </td>
      </tr>
    <% end %>
  </table>
</div>
